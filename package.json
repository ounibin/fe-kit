{
  "name": "gangu",
  "version": "0.2.0",
  "description": "gangu 是一款js工具库，封装了一些js开发过程中的常用方法",
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "test": "vitest",
    "docs:dev": "typedoc && vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "deploy": "npm login && pnpm install && pnpm build && pnpm docs:build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ounibin/gangu.git"
  },
  "author": "wangkj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ounibin/gangu/issues"
  },
  "homepage": "https://github.com/ounibin/gangu#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "rollup": "^4.14.3",
    "tslib": "^2.6.2",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typescript": "^5.1.3",
    "vitepress": "^1.1.0",
    "vitest": "^1.5.0"
  },
  "volta": {
    "node": "18.17.1"
  }
}